#include <reflowOS/boot/boot_memory.hpp> \

namespace reflowOS {
namespace boot 	   {

	template <typename ModuleType>
	ModuleType* load_module(ModuleType&&);

	template <typename ModuleType, typename ModuleCount>
	ModuleType* load_module(ModuleType(&)[ModuleCount]);

}
}

template <typename ModuleType>
ModuleType* reflowOS::boot::load_module(ModuleType&& init_module);

template <typename ModuleType, typename ModuleCount>
ModuleType* reflowOS::boot::load_module(ModuleType(&)[ModuleCount]);
