#include <reflowOS/base/boot/hardware_specific/microblaze/boot_data.hpp>


namespace reflowOS::system {
	static  mman_type* memory_manager;
}

namespace reflowOS::hardware_specific::microblaze {
	void start_boot()
	{
		memory_traits::initialize(); // Initializes Memory System.
		reflowOS::system::memory_manager = memory::basic_memory_manager<reflowOS::hardware_specific::microblaze::memory_traits>::initialize();
	}
}
