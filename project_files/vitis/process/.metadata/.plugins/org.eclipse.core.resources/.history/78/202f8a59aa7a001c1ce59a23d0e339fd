#include <xparameters.h>
#include <xil_printf.h>

struct pcb
{
	int registers[13];
	int ret_pointer  ;
	int stack_pointer;
};

void test()
{
	xil_printf  ("Hello Test #1");
}

int main()
{
	int a;
	asm volatile ("swi R21, R0, 3\r\n"
				  "addi R22, R2, -4\r\n"
				  "sw R22, R0, R21");

	xil_printf("Hello World\r\n");
	xil_printf("%d\r\n", a);
}
