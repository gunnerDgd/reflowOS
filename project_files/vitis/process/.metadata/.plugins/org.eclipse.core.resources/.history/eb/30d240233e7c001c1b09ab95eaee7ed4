#include <type_traits>

namespace reflowOS {
	template <>
	class basic_context<hardware_specific::microblaze::context>
	{
	public:
		typedef hardware_specific::microblaze::context context_type;

	public:
		void switch_to(ContextObject&&);

	private:
		context_type __M_context_entity;
	};
}
