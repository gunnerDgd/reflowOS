#pragma once

namespace reflow_bootloader {
namespace clocking 		    {

	struct prescaler {};
	template <std::uint32_t Multiplier> struct multiplier : public prescaler
	{
		static constexpr std::uint32_t value = Multiplier;
	};

	template <std::uint32_t Divider>    struct divider    : public prescaler
	{
		static constexpr std::uint32_t value = Divider   ;
	};

}
}
