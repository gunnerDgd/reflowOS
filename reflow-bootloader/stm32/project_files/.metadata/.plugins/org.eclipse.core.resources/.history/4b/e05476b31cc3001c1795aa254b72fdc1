#include "stm32f4xx_ll_rcc.h"

namespace reflow_bootloader {
namespace hardware_specific {
namespace clocking 			{

	using			 frequency_type		 = std::uint32_t;
	using 			 native_clock_source = std::uint32_t;

	inline constexpr native_clock_source internal_clock_handle  = LL_RCC_SYS_CLKSOURCE_HSI;
	inline constexpr native_clock_source external_clock_handle  = LL_RCC_SYS_CLKSOURCE_HSE;
	inline constexpr native_clock_source prescaled_clock_handle = LL_RCC_SYS_CLKSOURCE_PLL;

}
}
}
