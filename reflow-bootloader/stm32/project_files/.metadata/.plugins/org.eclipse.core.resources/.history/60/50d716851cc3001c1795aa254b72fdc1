#include <reflow_bootloader/clock/system_clock.hpp>

reflow_bootloader::clocking::system_clock::system_clock()
	: __M_sysclock_source(LL_RCC_GetSysClkSource()) {  }

typename reflow_bootloader::clocking::system_clock::raw_clock_type
		 reflow_bootloader::clocking::system_clock::current_frequency() { return LL_RCC_GetSystemClocksFreq(); }

typename reflow_bootloader::clocking::system_clock::raw_clock_type
		 reflow_bootloader::clocking::system_clock::native_handle    () { return __M_sysclock_source; }
